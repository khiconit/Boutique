{% assign total_image= 100 %} 
{% assign pagination_item  = 3  %}
<div class="container">
  {{ '47.jpg' | asset_url | img_tag }}
  <ul class="kt-popup-gallery lookbook-list row">
    {% assign count_image = 1 %}
    {% assign count_page =  0 %}
    {% assign page  =  1  %}
    {% for index_image in (1..total_image) %}
      {% assign product_item = 'img_lookbook' | append : index_image %}
      {% capture this_product_item %}{%include 'shortcode-render' render: product_item %}{% endcapture %}
         {% if this_product_item   != "" %}
          {% assign count_page = count_page | plus: 1   %}

          <li class="item col-sm-4 col-md-3 lookbook-item page{{page}}">
            <a href="{{this_product_item | file_url  }}">
              <img src="{{this_product_item | file_url  }}" alt="" />
            </a>
          </li>
          {% if  count_page >= pagination_item %}
            {% assign page  =page | plus:   1  %}
            {% assign count_page =  0 %}
          {% endif %}
          {% assign count_image = count_image | plus: 1  %}
          {% endif %}
      {% endfor %}
  </ul>
 
  <div class="navigation">
    <ul id="paginate-ul">
      {% if count_image > pagination_item  %}
       <li><a href="#" class="prev-book"><i class="fa fa-long-arrow-left"></i></a></li>
          {% assign total_page =  count_image  | divided_by: pagination_item | ceil  %}
            {% for index_page in (1..total_page)  %}
              <li {% if forloop.first  %} class="active" {% endif %}><a href="#" data-page="{{index_page}}" class="number-pagination">{{index_page}}</a></li>
            {% endfor %}
        {% endif %}
      <li > <a href="#" class="next-book"><i class="fa fa-long-arrow-right"></i></a></li>
    </ul>
  </div>
 
</div>