.white-popup {
	position: relative;
	background: $white;
	width: auto;
	width: 433px;
	margin: 20px auto;
}
.kt-popup-newsletter {
	padding: 20px 60px;
	text-align: center;
	padding-bottom: 100px;
	.popup-title {
		margin-top: 40px;
		text-transform: uppercase;
		p {
			margin: 0;
			margin-top: 20px;
		}
		h3 {
			font-size: 24px;
			margin-bottom: 15px;
		}
	}
	.form-subscribe {
		padding-top: 40px;
		.input {
			width: 100%;
			margin-bottom: 20px;
			font-family: $font_10, $font_11;
		}
		.button {
			margin: 0 5px;
		}
	}
	.checkbox {
		position: absolute;
		margin: 0;
		left: 20px;
		bottom: 10px;
		font-size: 13px;
		color: $color_suva_gray_approx;
		font-weight: normal;
		font-family: $font_10, $font_11;
		label {
			font-weight: normal;
		}
		input[type="checkbox"] {
			vertical-align: middle;
			margin-top: 6px;
		}
	}
	.primary {
		display: inline-block;
		background-color: $color_driftwood_approx;
		color: $white;
		padding: 0 10px;
	}
}
.bg-image {
	background-size: cover;
	background-position: center;
	background-repeat: no-repeat;
	width: 100%;
	min-height: 215px;
	display: table;
	font-size: 0;
	overflow: hidden;
	position: relative;
	&:before {
		content: '';
		position: absolute;
		top: 0;
		bottom: 0;
		right: 0;
		left: 0;
		//Instead of the line below you could use @include transition($transition-1, $transition-2, $transition-3, $transition-4, $transition-5, $transition-6, $transition-7, $transition-8, $transition-9, $transition-10)
		transition: all .6s ease-in-out;
	}
	&:after {
		content: '';
		position: absolute;
		top: 0;
		bottom: 0;
		right: 0;
		left: 0;
		//Instead of the line below you could use @include transition($transition-1, $transition-2, $transition-3, $transition-4, $transition-5, $transition-6, $transition-7, $transition-8, $transition-9, $transition-10)
		transition: all .6s ease-in-out;
	}
}
.block-kt-banner {
	position: relative;
	.banner-content {
		position: absolute;
		max-width: 100%;
	}
	&.style1 {
		.title {
			font-size: 50px;
			color: $color_sundance_approx;
			font-family: $font_1;
			line-height: normal;
			margin: 0;
		}
		.subtitle {
			font-size: 18px;
			text-transform: uppercase;
			letter-spacing: 3px;
			&:after {
				content: '';
				width: 36px;
				height: 2px;
				display: block;
				margin: 0 auto;
				background-color: $color_mine_shaft_approx;
				margin-top: 20px;
			}
		}
		.banner-button {
			margin-top: 15px;
			text-transform: uppercase;
			color: $color_mine_shaft_approx;
			display: inline-block;
			font-size: 13px;
			&:hover {
				color: $color_tussock_approx;
			}
		}
		&:hover {
			.title {
			}
			.subtitle {
			}
		}
	}
	&.style2 {
		.banner-content {
			border: 5px solid $color_tussock_approx;
			padding: 20px 0;
		}
		.title {
			font-size: 60px;
			color: $color_sundance_approx;
			font-family: $font_1;
			line-height: normal;
			margin: 0;
		}
		.subtitle {
			color: $white;
			font-family: $font_6;
			font-style: italic;
			font-weight: 700;
			font-size: 18px;
			.big-text {
				font-family: $font_5;
				font-style: normal;
				font-size: 30px;
				text-transform: uppercase;
				display: inline-block;
				margin-right: 10px;
			}
		}
		&:hover .banner-content {
		}
	}
	&.style3 {
		.banner-content {
			border: 5px solid $color_cabaret_approx;
			padding: 50px 30px 60px 75px;
			color: $white;
		}
		.title {
			color: $white;
			font-family: $font_1;
			font-size: 60px;
			margin: 0;
			line-height: 45px;
		}
