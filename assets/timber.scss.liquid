// ==============================================================================
// #Site Nav and Dropdowns
// ==============================================================================
.nav-bar {
	background-color: none;
	font-family: $navFont;

	ul {
		margin: 0;
	}

	li {
		margin: 0;
	}
}

.site-nav {
	font-size: 18px;
	cursor: default;
	float: left;

	li {
		margin: 0;
		display: block;
		
		&.hover {
			> a {
				color: $colorPrimary;
				border-bottom: 2px solid $colorPrimary;
				background:{{ settings.nav_hover_bgcolor }};
			}
		}
	}

	& > li {
		position: relative;
		display: inline-block;
		margin: 0 2px;
		float: left;
	}

	a {
		border-radius: 0;
		border-bottom: 2px solid transparent;
		color: {{ settings.nav_text_color }};
		display: block;
		font-weight: 300;
		height: {{ settings.header_height }};
		line-height: {{ settings.header_height }};
		padding: 0 $gutter;
		text-decoration: none;
		white-space: nowrap;
		-webkit-transition: all 0.3s ease;
		transition: all 0.3s ease;
	
		&:hover {
			color: $colorPrimary;
			border-bottom: 2px solid $colorPrimary;
			background:{{ settings.nav_hover_bgcolor }};
		}
	}
	li.parent.level-1 {
		position: relative;
		
		ul.site-nav--dropdown {
			//background: {{ settings.header_background }};
			//box-shadow: 0 3px 3px #ccc;
			//box-shadow: 0 3px 3px rgba(0, 0, 0, 0.2);
			left: 100%;
			margin-left: 30px;
			margin-top: 0;
			//min-width: 200px;
			opacity: 0;
			//position: absolute;
			top: -1px;
			visibility: hidden;
			//-webkit-transition: all 0.3s ease;
			//transition: all 0.3s ease;
		}
		
		&.hover {
			ul.site-nav--dropdown {
				opacity: 1;
				visibility: visible;
				margin-left: 0;
			}
		}
	}
	
	@include at-query ($max, $medium) {
		margin: 0 auto;
		text-align: center;
	}
	
}

.site-nav--active > a {
	color: $colorPrimary;
	border-bottom: 2px solid $colorPrimary;
}

// Dropdowns
.nav-bar {
	li {
		.site-nav--dropdown {
			background: {{ settings.header_background }};
			border: 1px solid $borderColor;
			box-shadow: 0 3px 3px #ccc;
			box-shadow: 0 3px 3px rgba(0, 0, 0, 0.2);
			display: block;
			left: 0;
			min-width: 200px;
			position: absolute;
			-webkit-transition: all 0.3s ease;
			transition: all 0.3s ease;
			visibility: hidden;
			z-index: 5;
			margin-top: 30px;
			opacity: 0;

			li {
				a {
					border-bottom: none;
					border-top: 1px solid $borderColor;
					color: {{ settings.body_text_color }};
					font-family: $bodyFontStack;
					font-size: 14px;
					height: 40px;
					line-height: 40px;

					&:hover,
					&:active {
						background-color: {{ settings.nav_hover_bgcolor }};
						
					}
				}
				&:first-child {
					> a {
						border-top: 1px solid transparent;
					}
				}
				&.active > a {
					background-color: {{ settings.nav_hover_bgcolor }};
				}
			}
		}
		&.hover {
			.site-nav--dropdown {
				display: block;
				visibility: visible;
				margin-top: 0px;
				opacity: 1;
			}
		}
	}
}

// Search bar in header
.nav-search {
	position: relative;
