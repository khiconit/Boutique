@media screen and (max-width: 1044px) {
	body {
		padding-top: 0;
	}
	.nav-toggler {
		border: 1px solid $borderColor;
		cursor: pointer;
		display: block;
		float: right;
		height: 40px;
		line-height: 38px;
		padding: 0;
		
		span {
			float: left;
			padding: 0 $gutter;
		}
		.fa {
			border-left: 1px solid $borderColor;
			float: right;
			font-size: 17px;
			line-height: 38px;
			padding: 0 15px;
			-webkit-transition: all 0.3s ease;
			transition: all 0.3s ease;
			
			&:hover {
				background: #f2f2f2;
			}
		}
	}
	.site-header, .site-header.smaller {
		height: {{ settings.header_height }};
		position: relative;
		
		.wrapper {
			height: {{ settings.header_height }};
			
			.grid--full {
				height: {{ settings.header_height }};
				
				.grid-table {
					height: {{ settings.header_height }};
					
					.grid-row {
						height: {{ settings.header_height }};
						
						.grid-cell {
							height: {{ settings.header_height }};
							
							&.desktop-nav {
								display: none;
							}
							
							.mobile-nav {
								display: block;
								float: right;
								margin-left: 10px;
								
								.site-nav {
									opacity: 0;
									visibility: hidden;
									-webkit-transition: all 0.3s ease;
									transition: all 0.3s ease;
								}
								
								&.open {
									.site-nav {
										opacity: 1;
										visibility: visible;
									}
								}
							}
							.nav-bar {
								float: left;
								height: 40px;
								position: relative;
								
								.site-nav {
									background: {{ settings.header_background }};
									border: 1px solid $borderColor;
									box-shadow: 0 3px 5px #ccc;
									box-shadow: 0 3px 5px rgba(0,0,0,0.3);
									position: absolute;
									right: 0;
									top: 42px;
									width: 300px;
									
									li {
										border-top: 1px solid #ddd;
										margin: 0;
										text-align: left;
										width: 100%;
										-webkit-transition: height 0.3s ease;
										transition: height 0.3s ease;
										
										a {
											font-size: 14px;
											height: 40px;
											line-height: 40px;
										}
										.site-nav--dropdown {
											border-bottom: medium none;
											border-left: medium none;
											border-right: medium none;
											box-shadow: none;
